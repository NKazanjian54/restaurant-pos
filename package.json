{
  "name": "restaurant-pos",
  "version": "1.0.0",
  "description": "Quick-service restaurant POS system",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install"
  },
  "devDependencies": {
    "concurrently": "^7.6.0"
  },
  "keywords": [
    "pos",
    "restaurant",
    "react",
    "nodejs",
    "sqlite"
  ],
  "author": "NKazanjian54",
  "license": "MIT"
}
